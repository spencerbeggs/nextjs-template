{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"dynamicImport": false,
			"decorators": false
		},
		"target": "es2018",
		"loose": true,
		"externalHelpers": false
	},
	"module": {
		"type": "es6",
		"noInterop": false
	},
	"isModule": true,
	"minify": false
}
