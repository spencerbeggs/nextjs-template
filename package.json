		"semver": "^7.3.5",
		"serve-static": "^1.14.1",
		"stylelint": "^14.1.0",
		"stylelint-config-recommended-scss": "^5.0.2",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.0.0",
		"tailwindcss": "^2.2.19",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.4.0",
