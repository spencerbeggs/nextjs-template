{
	"name": "next-template",
	"version": "1.3.0",
	"description": "Just a little boilerplate for setting up Typescript projects the way I like them.",
	"repository": "https://github.com/spencerbeggs/next-template.git",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"license": "MIT",
	"private": true,
	"engineStrict": true,
	"engines": {
		"node": "16.15.0"
	},
	"scripts": {
		"dev": "yarn compile && PORT=3001 APP_ENV=local node --no-warnings --loader ts-paths-esm-loader/transpile-only --experimental-vm-modules --experimental-specifier-resolution=node ./lib/scripts/server.ts",
		"build": "yarn compile && APP_ENV=production npx --node-options='--experimental-import-meta-resolve' next build",
		"compile": "npx --node-options='--experimental-import-meta-resolve --experimental-vm-modules' tsc -p tsconfig.build.json",
		"next": "next start",
		"engines": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./lib/scripts/engines.ts",
		"lint": "eslint . --ext .ts,.tsx,.cjs,.mjs,.js",
		"lint-fix": "eslint . --ext .ts,.tsx,.cjs,.mjs,.js --fix",
		"start": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node index.ts",
		"ssl": "node --no-warnings --loader ts-node/esm --experimental-import-meta-resolve --experimental-specifier-resolution=node ./lib/scripts/ssl.ts",
		"test": "jest",
		"stylelint-check": "stylelint-config-prettier-check",
		"test-ci": "TEST_ENV=ci jest --ci",
		"travis": "jest --coverage --coverageReporters=text-lcov | coveralls"
	},
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@next/eslint-plugin-next": "12.1.6",
		"@octokit/action": "^3.18.1",
		"@octokit/rest": "^18.12.0",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"@types/finalhandler": "^1.1.1",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^27.5.1",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^17.0.35",
		"@types/pem": "^1.9.6",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.5",
		"@types/redux-logger": "^3.0.9",
		"@types/semver": "^7.3.9",
		"@types/serve-static": "^1.13.10",
		"@types/ua-parser-js": "^0.7.36",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"autoprefixer": "^10.4.7",
		"chalk": "^5.0.1",
		"css-loader": "^6.7.1",
		"dotenv": "^16.0.1",
		"dotenv-expand": "^8.0.3",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"extract-css-chunks-webpack-plugin": "^4.9.0",
		"file-loader": "^6.2.0",
		"finalhandler": "^1.2.0",
		"fs-extra": "^10.1.0",
		"image-webpack-loader": "^8.1.0",
		"jest": "^28.1.0",
		"jest-console": "^0.1.0",
		"pem": "^1.14.6",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-preset-env": "^7.6.0",
		"prettier": "^2.6.2",
		"prettier-plugin-tailwindcss": "^0.1.11",
		"semver": "^7.3.7",
		"serve-static": "^1.15.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.8.5",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-order": "^5.0.0",
		"stylelint-prettier": "^2.0.0",
		"tailwindcss": "^3.0.24",
		"ts-jest": "^28.0.3",
		"ts-node": "^10.8.0",
		"ts-paths-esm-loader": "^1.3.0",
		"tsconfig-paths": "^4.0.0",
		"type-fest": "^2.13.0",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.8.2",
		"@sindresorhus/slugify": "^2.1.0",
		"@use-it/event-listener": "^0.1.7",
		"connected-next-router": "^4.1.1",
		"lodash-es": "^4.17.21",
		"next": "^12.1.7-canary",
		"next-pwa": "^5.5.2",
		"next-redux-wrapper": "^7.0.5",
		"postcss": "^8.4.14",
		"postcss-import": "^14.1.0",
		"postcss-nested": "^5.0.6",
		"postcss-scss": "^4.0.4",
		"postcss-url": "^10.1.3",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-redux": "^8.0.2",
		"redux": "^4.2.0",
		"redux-devtools-extension": "^2.13.9",
		"redux-logger": "^3.0.6",
		"redux-promise-middleware": "^6.1.2",
		"redux-thunk": "^2.4.1",
		"ua-parser-js": "^1.0.2",
		"webpack": "^5.72.1"
	}
}
