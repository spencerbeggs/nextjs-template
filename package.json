{
	"name": "next-template",
	"version": "1.3.0",
	"description": "Just a little boilerplate for setting up Typescript projects the way I like them.",
	"repository": "https://github.com/spencerbeggs/next-template.git",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"license": "MIT",
	"private": true,
	"engineStrict": true,
	"engines": {
		"node": "16.13.1"
	},
	"scripts": {
		"dev": "PORT=3001 APP_ENV=local node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node lib/scripts/server.ts",
		"build": "npx --node-options='--experimental-import-meta-resolve' next build",
		"next": "next start",
		"engines": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./lib/scripts/engines.ts",
		"lint": "eslint . --ext .ts,.tsx,.cjs,.mjs,.js",
		"lint-fix": "eslint . --ext .ts,.tsx,.cjs,.mjs,.js --fix",
		"start": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node index.ts",
		"ssl": "node --no-warnings --loader ts-node/esm --experimental-import-meta-resolve --experimental-specifier-resolution=node ./lib/scripts/ssl.ts",
		"test": "jest",
		"test-ci": "TEST_ENV=ci jest --ci",
		"travis": "jest --coverage --coverageReporters=text-lcov | coveralls"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@next/eslint-plugin-next": "12.0.7",
		"@octokit/action": "^3.18.0",
		"@octokit/rest": "^18.12.0",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/line-clamp": "^0.3.0",
		"@tailwindcss/typography": "^0.5.0",
		"@types/finalhandler": "^1.1.1",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^27.0.3",
		"@types/lodash-es": "^4.17.5",
		"@types/node": "^17.0.7",
		"@types/pem": "^1.9.6",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/redux-logger": "^3.0.9",
		"@types/semver": "^7.3.9",
		"@types/serve-static": "^1.13.10",
		"@types/ua-parser-js": "^0.7.36",
		"@types/ws": "^8.2.2",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"autoprefixer": "^10.4.1",
		"chalk": "^5.0.0",
		"css-loader": "^6.5.1",
		"dotenv": "^10.0.0",
		"dotenv-expand": "^5.1.0",
		"eslint": "^8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"extract-css-chunks-webpack-plugin": "^4.9.0",
		"file-loader": "^6.2.0",
		"finalhandler": "^1.1.2",
		"fs-extra": "^10.0.0",
		"image-webpack-loader": "^8.0.1",
		"jest": "^27.4.5",
		"jest-console": "^0.1.0",
		"pem": "^1.14.4",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-preset-env": "^7.2.0",
		"prettier": "^2.5.1",
		"sass": "^1.45.1",
		"sass-loader": "^12.4.0",
		"sass-resources-loader": "^2.2.4",
		"semver": "^7.3.5",
		"serve-static": "^1.14.2",
		"style-loader": "^3.3.1",
		"stylelint": "^14.2.0",
		"stylelint-config-recommended-scss": "^5.0.2",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.1.0",
		"tailwindcss": "^3.0.8",
		"ts-jest": "^27.1.2",
		"ts-node": "^10.4.0",
		"tsconfig-paths": "^3.12.0",
		"type-fest": "^2.8.0",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"@sindresorhus/slugify": "^2.1.0",
		"@use-it/event-listener": "^0.1.7",
		"connected-next-router": "^4.0.1",
		"lodash-es": "^4.17.21",
		"next": "^12.0.7",
		"next-redux-wrapper": "^7.0.5",
		"postcss": "^8.4.5",
		"postcss-import": "^14.0.2",
		"postcss-nested": "^5.0.6",
		"postcss-scss": "^4.0.2",
		"postcss-url": "^10.1.3",
		"react": "^18.0.0-rc.0",
		"react-dom": "^18.0.0-rc.0",
		"react-redux": "^7.2.6",
		"redux": "^4.1.2",
		"redux-devtools-extension": "^2.13.9",
		"redux-logger": "^3.0.6",
		"redux-promise-middleware": "^6.1.2",
		"redux-thunk": "^2.4.1",
		"ua-parser-js": "^1.0.2",
		"webpack": "^5.65.0"
	}
}
