{
	"name": "next-template",
	"version": "1.3.0",
	"description": "Just a little boilerplate for setting up Typescript projects the way I like them.",
	"repository": "https://github.com/spencerbeggs/next-template.git",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"license": "MIT",
	"private": true,
	"engineStrict": true,
	"engines": {
		"node": "16.x"
	},
	"scripts": {
		"dev": "yarn compile && PORT=3001 APP_ENV=local node --no-warnings --loader ts-node/esm --experimental-import-meta-resolve --experimental-vm-modules --experimental-specifier-resolution=node ./lib/scripts/server.ts",
		"build": "yarn compile && APP_ENV=production npx --node-options='NODE_ENV=production --experimental-import-meta-resolve' next build",
		"compile": "yarn run swc next.config.ts -o next.config.mjs --config-file .swc.next.config -s inline",
		"engines": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./lib/scripts/engines.ts",
		"lint": "eslint . --ext .ts,.tsx,.cjs,.mjs,.js",
		"lint-fix": "eslint . --ext .ts,.tsx,.cjs,.mjs,.js --fix",
		"prod": "yarn build && yarn start",
		"start": "NODE_OPTIONS='NODE_ENV=production' next start -p 3001",
		"ssl": "node --no-warnings --loader ts-node/esm --experimental-import-meta-resolve --experimental-specifier-resolution=node ./lib/scripts/ssl.ts",
		"test": "jest",
		"stylelint-check": "stylelint-config-prettier-check",
		"test-ci": "TEST_ENV=ci jest --ci",
		"travis": "jest --coverage --coverageReporters=text-lcov | coveralls"
	},
	"devDependencies": {
		"@babel/core": "^7.19.1",
		"@next/eslint-plugin-next": "12.3.1",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.2",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.7",
		"@types/finalhandler": "^1.2.0",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^29.0.3",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^18.7.18",
		"@types/pem": "^1.9.6",
		"@types/postcss-import": "^14.0.0",
		"@types/react": "^18.0.20",
		"@types/react-dom": "^18.0.6",
		"@types/redux-logger": "^3.0.9",
		"@types/semver": "^7.3.12",
		"@types/serve-static": "^1.15.0",
		"@types/ua-parser-js": "^0.7.36",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"autoprefixer": "^10.4.12",
		"browserslist": "^4.21.4",
		"chalk": "^5.0.1",
		"css-loader": "^6.7.1",
		"dotenv": "^16.0.2",
		"dotenv-expand": "^9.0.0",
		"dotenv-loader": "^2.0.1",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.5.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"extract-css-chunks-webpack-plugin": "^4.9.0",
		"file-loader": "^6.2.0",
		"finalhandler": "^1.2.0",
		"fs-extra": "^10.1.0",
		"image-webpack-loader": "^8.1.0",
		"nanoid": "^4.0.0",
		"pem": "^1.14.6",
		"postcss": "^8.4.16",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-import": "^15.0.0",
		"postcss-nested": "^5.0.6",
		"postcss-preset-env": "^7.8.2",
		"postcss-scss": "^4.0.5",
		"postcss-url": "^10.1.3",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"semver": "^7.3.7",
		"serve-static": "^1.15.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.12.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-order": "^5.0.0",
		"stylelint-prettier": "^2.0.0",
		"tailwindcss": "^3.1.8",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0",
		"type-fest": "^3.0.0",
		"typescript": "^4.8.3",
		"webpack": "^5.74.0"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.2",
		"@heroicons/react": "^2.0.11",
		"@next-safe/middleware": "^0.10.0",
		"@reduxjs/toolkit": "^1.8.5",
		"@sindresorhus/slugify": "^2.1.0",
		"@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
		"eslint-plugin-unused-imports": "^2.0.0",
		"lodash-es": "^4.17.21",
		"next": "^12.3.1",
		"next-pwa": "^5.6.0",
		"next-redux-wrapper": "^8.0.0",
		"postcss-nesting": "^10.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.2",
		"redux": "^4.2.0",
		"redux-devtools-extension": "^2.13.9",
		"redux-logger": "^3.0.6",
		"redux-promise-middleware": "^6.1.2",
		"redux-thunk": "^2.4.1",
		"ua-parser-js": "^1.0.2"
	}
}
